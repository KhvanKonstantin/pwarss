{
  "name": "pwarss",
  "version": "0.0.1",
  "author": "Konstantin Khvan",
  "license": "Apache-2.0",
  "description": "Frontend of pwarss",
  "repository": "https://github.com/KhvanKonstantin/pwarss",
  "bugs": "https://github.com/KhvanKonstantin/pwarss/issues",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.3",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.17",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "mobx": "^5.15.0",
    "mobx-react": "^6.1.4",
    "normalize.css": "^8.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "typescript": "^3.7.3"
  },
  "scripts": {
    "start-ts": "CHOKIDAR_USEPOLLING=true CHOKIDAR_INTERVAL=1000 react-scripts start",
    "build-ts": "react-scripts build",
    "build-css": "node-sass-chokidar --include-path ./src src/index.scss -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --use-polling --polling-interval 1000 --include-path ./src src/index.scss -o src/ --watch",
    "start": "npm-run-all -p watch-css start-ts",
    "build": "npm-run-all build-css build-ts",
    "test": "CI=true react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.5"
  },
  "proxy": "http://localhost:8080",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
